<div class="container">
    <div style="height: 20%;">
        <h1 align="Center">Vista del drone</h1>
    </div>
    <div style="height: 60%;border-style:dotted;">
        <img id="play" width="100%" height="100%">
    </div>
    <div style="height: 20%;">
        <h1 align="Center">Opciones</h1>
    </div>
</div>
<script>
    var socket = io();
    socket.on('flujo', (image)=>{
        var img = document.getElementById("play");
        img.src = image;
    });
</script>
